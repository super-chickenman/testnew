               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <TextField fx:id="a_main_password" editable="false" prefHeight="30.0" prefWidth="167.0" />
            <Button fx:id="btn_alter" mnemonicParsing="false" onAction="#AlterEvent" prefHeight="37.0" prefWidth="86.0" text="修改密码">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="btn_cancel" alignment="CENTER" mnemonicParsing="false" onAction="#CancelEvent" prefHeight="39.0" prefWidth="86.0" text="取消" visible="false">
