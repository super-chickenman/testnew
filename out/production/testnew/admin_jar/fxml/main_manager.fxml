<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="admin_jar.font.MainController">
   <children>
      <VBox prefHeight="400.0" prefWidth="167.0">
         <children>
            <Label alignment="CENTER_RIGHT" contentDisplay="CENTER" prefHeight="62.0" prefWidth="169.0" text="个人信息">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label prefHeight="41.0" prefWidth="57.0" text="(1)账号">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <TextField fx:id="a_main_aid" editable="false" prefHeight="30.0" prefWidth="167.0" />
            <Label prefHeight="43.0" prefWidth="52.0" text="(2)姓名">
