            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="btn_cancel" layoutX="273.0" layoutY="118.0" mnemonicParsing="false" onAction="#CancelEvent" prefHeight="39.0" prefWidth="67.0" text="取消">
